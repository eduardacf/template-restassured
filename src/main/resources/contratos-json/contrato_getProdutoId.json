{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "id",
        "title",
        "description",
        "price",
        "discountPercentage",
        "rating",
        "stock",
        "brand",
        "category",
        "thumbnail",
        "images"
    ],
    "properties": {
        "id": {
            "type": "integer",
            "default": 0,
            "title": "The id Schema",
            "examples": [
                1
            ]
        },
        "title": {
            "type": "string",
            "default": "",
            "title": "The title Schema",
            "examples": [
                "iPhone 9"
            ]
        },
        "description": {
            "type": "string",
            "default": "",
            "title": "The description Schema",
            "examples": [
                "An apple mobile which is nothing like apple"
            ]
        },
        "price": {
            "type": "integer",
            "default": 0,
            "title": "The price Schema",
            "examples": [
                549
            ]
        },
        "discountPercentage": {
            "type": "number",
            "default": 0.0,
            "title": "The discountPercentage Schema",
            "examples": [
                12.96
            ]
        },
        "rating": {
            "type": "number",
            "default": 0.0,
            "title": "The rating Schema",
            "examples": [
                4.69
            ]
        },
        "stock": {
            "type": "integer",
            "default": 0,
            "title": "The stock Schema",
            "examples": [
                94
            ]
        },
        "brand": {
            "type": "string",
            "default": "",
            "title": "The brand Schema",
            "examples": [
                "Apple"
            ]
        },
        "category": {
            "type": "string",
            "default": "",
            "title": "The category Schema",
            "examples": [
                "smartphones"
            ]
        },
        "thumbnail": {
            "type": "string",
            "default": "",
            "title": "The thumbnail Schema",
            "examples": [
                "https://i.dummyjson.com/data/products/1/thumbnail.jpg"
            ]
        },
        "images": {
            "type": "array",
            "default": [],
            "title": "The images Schema",
            "items": {
                "type": "string",
                "title": "A Schema",
                "examples": [
                    "https://i.dummyjson.com/data/products/1/1.jpg",
                    "https://i.dummyjson.com/data/products/1/2.jpg",
                    "https://i.dummyjson.com/data/products/1/3.jpg",
                    "https://i.dummyjson.com/data/products/1/4.jpg",
                    "https://i.dummyjson.com/data/products/1/thumbnail.jpg"
                ]
            },
            "examples": [
                ["https://i.dummyjson.com/data/products/1/1.jpg",
                    "https://i.dummyjson.com/data/products/1/2.jpg",
                    "https://i.dummyjson.com/data/products/1/3.jpg",
                    "https://i.dummyjson.com/data/products/1/4.jpg",
                    "https://i.dummyjson.com/data/products/1/thumbnail.jpg"
                ]
            ]
        }
    },
    "examples": [{
        "id": 1,
        "title": "iPhone 9",
        "description": "An apple mobile which is nothing like apple",
        "price": 549,
        "discountPercentage": 12.96,
        "rating": 4.69,
        "stock": 94,
        "brand": "Apple",
        "category": "smartphones",
        "thumbnail": "https://i.dummyjson.com/data/products/1/thumbnail.jpg",
        "images": [
            "https://i.dummyjson.com/data/products/1/1.jpg",
            "https://i.dummyjson.com/data/products/1/2.jpg",
            "https://i.dummyjson.com/data/products/1/3.jpg",
            "https://i.dummyjson.com/data/products/1/4.jpg",
            "https://i.dummyjson.com/data/products/1/thumbnail.jpg"
        ]
    }]
}